{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "stripInternal": true,
    "outDir": "./lib"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
